   "source": [
    "## **Move Zeroes**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def movezeroes(nums):\n",
    "    a = 0\n",
    "\n",
    "    for num in nums:\n",
    "        if num != 0:\n",
    "            nums[a] = num\n",
    "            a += 1\n",
    "\n",
    "    while a < len(nums):\n",
    "        nums[a] = 0\n",
    "        a += 1\n",
    "\n",
    "    return nums"
   ]
  },
